-//test for basic stuff

// Create Sphere 
polySphere -r 1 -sx 20 -sy 20 -ax 0 1 0 -cuv 2 -ch 1;

//Select Sphere
select -r pSphere1; 

//Rename Sphere
rename "pSphere1" "Test_Orb"

//Move Sphere
move -r 0 0 0 ;

//Scale Sphere
scale -r 0 0 0 ;

// Get everything selected
string $sel[] = `ls -sl`;

// Create out loop
for ($each in $sel) {
    select $each;
}
